@model OpenPac.Models.Education

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>Education</legend>

    <div class="display-label">School</div>
    <div class="display-field">@Model.School</div>

    <div class="display-label">Start</div>
    <div class="display-field">@String.Format("{0:g}", Model.Start)</div>

    <div class="display-label">End</div>
    <div class="display-field">@String.Format("{0:g}", Model.End)</div>

    <div class="display-label">Major</div>
    <div class="display-field">@Model.Major</div>

    <div class="display-label">Minor</div>
    <div class="display-field">@Model.Minor</div>

    <div class="display-label">Degree</div>
    <div class="display-field">@Model.Degree</div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.Id }) |
    @Html.ActionLink("Back to List", "Index")
</p>


